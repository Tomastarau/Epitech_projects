#include <stdio.h>

int my_strlen(char const *str);

int my_strncmp(char const *s1, char const *s2, int n)
{
    int str1 = 0;
    int str2 = 0;
    int i = 0;
    while (i < n){
        str1 = str1 + s1[i];
        str2 = str2 + s2[i];
        i = i + 1;
    }    
    if (str1 == str2){
        return (0);
    }
    printf("%d\n", str1);
    if (str1 < str2){
        printf("%d\n", str1);
        return (str1 - str2);
    } else {
        printf("%d\n", str1);
        return (str1 - str2);
    }
}
